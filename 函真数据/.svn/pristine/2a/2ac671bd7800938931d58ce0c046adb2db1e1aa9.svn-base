*{
	margin: 0; padding:0;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
body h3{
	font-size: 16px;
}
#top_bar{
	background-color: #303030;
}
body{
	background-color: #3d3d3d;
	color: #acacac;
	position: absolute;
	width: 100%;
	height: 100%;
	font-size: 12px;
}
h1, h2, h3{
	color: #dadada;
}
a, a:link, a:hover{
	color: inherit;
	text-decoration: none;
}
.function-title {
	width: 100%;
	height: 44px;
	background-color: #343434;
	position: absolute;
	top: 0;
	left: 0;
	color: #ccc;
	line-height: 44px;
	font-size: 16px;
}
.function-title a {
	color: #ccc;
	display: inline-block;
	width: 40px;
	position: relative;
}
.function-title a:after {
	position: absolute;
	top: 13px;
	left: 30px;
	display: inline-block;
	content: "";
	width: 1px;
	height: 20px;
	background-color: #969696;
}
.function-title span {
	font-size: 14px;
}
.part2{
	margin-top: 44px;
}
#content .am-nav, #content .am-nav *, #content .am-nav a{
	border: none;
	background-color: inherit;
	color: inherit;

}
#content .am-nav li:hover, #content .am-nav li.am-active{
	color: #dadada;
}
#content .am-nav li{
	width: 50%;
	text-align: center;

}
#content .am-nav li:first-child a{
	border-right: 1px solid #444;
}
#content .am-tabs-bd{
	border: none;
	margin: 5px;
}
ï¼ƒcontent ul{
	list-style: none;
}
#content .am-tab-panel *{
	margin: 0;
	padding: 0;
}
#content .am-tab-panel ul{
	position: relative;
	width: 100%;
}
#tab1 li.row{
	border-top: 1px solid #444;
	border-bottom: 1px solid #444;
}
#tab1 li.row>div{
	position: relative;
}
#operate{
	display: inline-block;
	position: absolute;
	right: 10%;
}
#operate>span{
	background-color: #666;
	padding: 3px 3px !important;
	-webkit-border-radius:3px;
	-moz-border-radius:3px;
	border-radius:3px;
}
#operate>span>span{
	margin: 0 2px !important;
}
.color-red{
	color: #E74C3C;
}
.color-green{
	color: #27ae60;
}
.color-brown{
	color: #E67E22;
}
.color-red-bg, .added{
	background-color: #E74C3C !important;
}
.color-green-bg{
	background-color: #27ae60;
}
.color-brown-bg{
	background-color: #E67E22;
}
#operate>span{
	cursor: pointer;
}
#operate .added{
	cursor: not-allowed;
}