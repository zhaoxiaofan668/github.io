body {
  background-color: #3d3d3d;
  font-size: 14px;
  padding: 0;
  margin: 0;
}
body .search {
  width: 100%;
  margin-top: 20px;
  background-color: #747680;
  border-radius: 10px;
}
body .search input {
  height: 40px;
  border: 0;
  width: 80%;
  padding: 0;
  border-radius: 5px;
  background-color: #747680;
  color: white;
  outline: none;
  border: none;
}
body .search span {
  width: 20%;
  /*height: 40px;*/
  font-size: 16px;
  text-align: center;
}
.container{
  padding-top: 44px;
}
.container .details {
  margin: 20px 0 10px 0;
  font-size: 1.4rem;
}
.container .details h2{
  color: white;
}
.container .details ul:first-child {
  margin-top: 20px;
}

.container .details ul:nth-of-type(even) {
  background-color: #454545;
}

.container .details ul {
  padding: 2px 0 1px 0;
  border-top: 1px solid #444;
  border-bottom: none;
}

.container .details ul li {
  text-align: center;
}

.container .details ul li span {
  color: #979797;
  position: relative;
  font-size: 1.2rem;
}

.container .details ul li span span {
  font-size: 14px;
  position: relative;
  top: -5px;
  color: #eee;
}
.tpm{
  color: white;
}

.function-title {
  width: 100%;
  height: 44px;
  background-color: #343434;
  position: absolute;
  top: 0;
  left: 0;
  color: #ccc;
  line-height: 44px;
  font-size: 1.6rem;
}
.function-title a {
  color: #ccc;
  display: inline-block;
  width: 40px;
  position: relative;
}
.function-title a:after {
  position: absolute;
  top: 13px;
  left: 30px;
  display: inline-block;
  content: "";
  width: 1px;
  height: 20px;
  background-color: #969696;
}
.function-title span {
  font-size: 1.4rem;
  margin-right: 20px;
  width: 50px;
}
.function-title .color-red {
  color: #E74D3D;
}
