#top_bar{
    position: relative;
    top:0;
    left:0;
    width: 100%;
    height: 50px;
    background-color: #3d3d3d;
    line-height: 50px;
}
#top_bar h1, #top_bar h2{
    color: #dadada;
    display:inline-block;
    font-weight: 600;
    position: absolute;
    margin: 0;
}
#top_bar h1{
    font-size: 20px;
    left: 50px;padding: 0 20px;
}
#top_bar h2{
    font-size: 12px;

    right: 5%;
}
#top_bar .back{
    display: inline-block;
    height: 100%;
    color: #dadada;
    width: 50px;
    left: 0;
    position: absolute;
    text-align: center;
    cursor: pointer;
}
#top_bar .bar{
    color: #444;
}